    /* Editor Colors
     * The most prominent editor colors are the token colors that are
     * based on the language grammar installed. These colors are defined
     * by the Color Theme (see below). All other colors are listed here.
     */
    //-: Editor Base Colors
    "editor.background": "#0a0e14",
    "editor.foreground": "#b3b1ad",
    "editorCursor.background": "#ff0000",
    "editorCursor.foreground": "#e6b450",
    "editorLineNumber.activeForeground": "#4d5566e6",
    "editorLineNumber.foreground": "#4d556699",
    "editorPane.background": "#ff0000",

    //-: Selection Colors - Visible when selecting one or more characters.
    "editor.inactiveSelectionBackground": "#1b2733",
    "editor.selectionBackground": "#273747",
    "editor.selectionForeground": "#ff0000",
    "editor.selectionHighlightBackground": "#1b2733",
    "editor.selectionHighlightBorder": "#304357",

    //-: Word Highlight Colors - Visible when the cursor is inside a symbol or word.
    "editor.wordHighlightBackground": "#6994bf12",
    "editor.wordHighlightBorder": "#6994bf80",
    "editor.wordHighlightStrongBackground": "#91b36212",
    "editor.wordHighlightStrongBorder": "#91b36280",

    //-: Find Colors - Depend on the current find string in the Find/Replace dialog.
    "editor.findMatchBackground": "#e6b4500d",
