    "tab.activeModifiedBorder": "#ff0000",
    "tab.border": "#0a0e14",
    "tab.hoverBackground": "#ff0000",
    "tab.hoverBorder": "#ff0000",
    "tab.hoverForeground": "#ff0000",
    "tab.inactiveBackground": "#0a0e14",
    "tab.inactiveForeground": "#4d5566",
    "tab.inactiveModifiedBorder": "#ff0000",
    "tab.lastPinnedBorder": "#ff0000",
    "tab.unfocusedActiveBackground": "#ff0000",
    "tab.unfocusedActiveBorder": "#4d5566",
    "tab.unfocusedActiveBorderTop": "#ff0000",
