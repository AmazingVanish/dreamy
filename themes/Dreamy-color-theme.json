    //-: Whitespace - To view the white spaces, enable Toggle Render Whitespace
    "editorWhitespace.foreground": "#4d556699",

    //-: Indent Guides - to view set "editor.renderIndentGuides": true
    "editorIndentGuide.activeBackground": "#4d5566b3",
    "editorIndentGuide.background": "#4d556666",

    //-: Rulers - to view rulers, define their location with "editor.rulers"
    "editorRuler.foreground": "#4d556666",
    "editor.linkedEditingBackground": "#ff0000",

    //-: CodeLens
    "editorCodeLens.foreground": "#626a73",

    //-: Lightbulb
