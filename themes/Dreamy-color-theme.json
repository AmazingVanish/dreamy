    "searchEditor.textInputBorder": "#ff0000",

    //-: Hover Highlight - Shown behind the symbol for which a hover is shown.
    "editor.hoverHighlightBackground": "#ff0000",

    //=: Current Line - Typically shown as a highlight or a border (not both)
    "editor.lineHighlightBackground": "#00010a",
    "editor.lineHighlightBorder": "#ff0000",

    //-: Link Color - Visible when clicking a link.
    "editorLink.activeForeground": "#e6b450",

    //-: Range Highlight - Visible when selecting a search result.
    "editor.rangeHighlightBackground": "#00010a",
    "editor.rangeHighlightBorder": "#ff0000",

    //-: Symbol Highlight - Visible when navigating to a symbol via a command such as Go to Definition
    "editor.symbolHighlightBackground": "#ff0000",
