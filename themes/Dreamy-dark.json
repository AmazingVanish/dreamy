    "tokenColors": [
        {
            "name": "Changed",
            "scope": [
                "markup.changed",
                "meta.diff.header.git",
                "meta.diff.header.from-file",
                "meta.diff.header.to-file"
            ],
            "settings": {
                "foreground": "#a2bffc",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Deleted",
            "scope": "markup.deleted.diff",
            "settings": {
                "foreground": "#EF535090",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Inserted",
            "scope": "markup.inserted.diff",
            "settings": {
                "foreground": "#c5e478ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Global settings",
            "settings": {
                "foreground": "#d6deeb"
            }
        },
        {
            "name": "Comment",
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#626a73"
            }
        },
        {
            "name": "String",
            "scope": ["string", "constant.other.symbol"],
